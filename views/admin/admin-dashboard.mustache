{{! add courses modal }}
<div class="modal fade" id="courseModal" tabindex="-1" role="dialog" aria-labelledby="coursesModal"
  aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content p-3">
      <div class="modal-header">
        <h5 class="modal-title">Add new course</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form id = "addCourseForm" class = "w-100">
            <input type = "hidden" id = "courseMode" value = "add"/>
            <input type = "hidden" id = "courseId" />
            <div class="mb-3 md-form">
                <label for="title">Title</label>
                <input type="text" name="title" class="form-control" required>
            </div>
            <div class="mb-3 md-form">
                <div class="custom-file">
                    <input id="courseImage" type="file" class="custom-file-input">
                    <label class="custom-file-label" for="courseImage" data-browse="Upload">Choose Course Image</label>
                </div>
            </div>
            <div class="mb-3 md-form">
                <label for="title">Price</label>
                <input type="number" name="price" class="form-control" required>
            </div>

            <div class="md-form mb-3">
              <textarea id="desc" name="details" class="md-textarea form-control" rows="3" required></textarea>
              <label for="desc">Description</label>
            </div>

            <div class="md-form">
              <span class="text-muted">Tags</span>
              <select class="tags form-control mt-2" id="tags" name="tags[]" multiple="multiple" style = "width: 100%" required>
                  {{{tags}}}
              </select>
            </div>
            
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
        <button type="button" id = "submitCourseFormButton" class="btn btn-default">Add Course</button>
      </div>
    </div>
  </div>
</div>

{{! add lawyer modal }}
<div class="modal fade" id="lawyerModal" tabindex="-1" role="dialog" aria-labelledby="lawyerModalLabel"
  aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="lawyerModalLabel">Send Lawyer Invite</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
          <div class="card-body px-lg-5 pt-0">
            <form class="text-center" style="color: #757575;" id="lawyerInvite">
                <div class="form-row">
                    <div class="col">
                        <div class="md-form">
                            <input type="text" id="fname" name="firstname" class="form-control">
                            <label for="fname">First name</label>
                        </div>
                    </div>
                    <div class="col">
                        <div class="md-form">
                            <input type="text" id="lname" name="lastname" class="form-control">
                            <label for="lname">Last name</label>
                        </div>
                    </div>
                </div>
                
                <div class="md-form mt-0">
                    <input type="email" id="mail" name="email" class="form-control">
                    <label for="email">E-mail</label>
                </div>

                <button class="btn btn-default btn-rounded btn-block my-4 waves-effect z-depth-0" id="sendInvite" type="submit">Send</button>

                <hr>

            </form>
          </div>
      </div>
      <div class="modal-footer">
        <button type="button" id="close" class="btn btn-danger" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>

