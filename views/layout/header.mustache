<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

  <!-- Font -->
  <link href="https://fonts.googleapis.com/css?family=Lato&display=swap" rel="stylesheet"> 
  <link href="https://fonts.googleapis.com/css2?family=Alegreya+Sans:wght@400&family=Poppins&display=swap" rel="stylesheet">
  <!-- Font Awesome -->
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css">
  <!-- Data-aos -->
  <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
  <!-- Bootstrap core CSS -->
  <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.3.1/css/bootstrap.min.css" rel="stylesheet">

  {{! Bootstrap multi select }}
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.13.1/css/bootstrap-select.css" />
  <!-- Material Design Bootstrap -->
  <link href="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.8.10/css/mdb.min.css" rel="stylesheet">

  <link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-notify/0.2.0/css/bootstrap-notify.css" rel="stylesheet">

  {{! select2 }}
  <link href="https://cdn.jsdelivr.net/npm/select2@4.0.12/dist/css/select2.min.css" rel="stylesheet" />

  {{! flatpickr datetime }}
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/flatpickr/dist/flatpickr.min.css">
  
  <!-- Material Design Bootstrap -->
  <link href="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.8.10/css/mdb.min.css" rel="stylesheet">
  
  <style type="text/css">
    :root {
      --accent-colr: #F8B24F;
      --main-font-family: 'Alegreya Sans', sans-serif;
      --sec-font-family: 'Poppins', sans-serif;
    }
    h1, h2, h3, h4, h5, h6 {
      font-family: var(--main-font-family);
      font-weight: bolder;
    }
    .terms-conditions a:hover {
      color: var(--accent-colr);
    }
    html,
    body p {
      font-family: var(--sec-font-family);
    }
    body {scroll-behavior: smooth;  margin:0;padding:0; overflow-x: hidden}
    div.parent-box { display: table; width:100%;}
    div.child-box { display: table-cell; vertical-align: middle; font-size: xx-large; }
    #client--main .nav-link.active {background-color: #49bcad;}
    @media only screen and (max-width: 600px) {
      #client--dashboard__links > a { display: inline; }
    }

/* client chat */
.client__chat .container {
  border: 2px solid #dedede;
  background-color: #f1f1f1;
  border-radius: 5px;
  padding: 10px;
  margin: 10px 0;
}

.client__chat .darker {
  border-color: #ccc;
  background-color: #ddd;
}

.client__chat .container::after {
  content: "";
  clear: both;
  display: table;
}

.client__chat .container img {
  float: left;
  max-width: 60px;
  width: 100%;
  margin-right: 20px;
  border-radius: 50%;
}

.client__chat .container img.right {
  float: right;
  margin-left: 20px;
  margin-right:0;
}

.client__chat .time-right {
  float: right;
  color: #aaa;
}

.client__chat .time-left {
  float: left;
  color: #999;
}
/* end client chat */

.read-color {
  color: white;
  background-color: rgb(63, 209, 190);
}
  </style>

  <script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.7.0/moment.min.js" type="text/javascript"></script>

  <title> {{title}} </title>
</head>
<body>
